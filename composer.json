{
    "name": "codicastudio/image",
    "description": "A random Codica Studio package.",
    "homepage": "https://github.com/codicastudio/image",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael J Brown",
            "email": "mbrown@codicastudio.com",
            "homepage": "https://codicastudio.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-exif": "*",
        "ext-mbstring": "*",
        "league/glide": "^1.4",
        "codicastudio/image-optimizer": "^1.0",
        "codicastudio/temporary-directory": "^1.0",
        "symfony/process": "^3.0|^4.0|^5.0"
    },
    "require-dev": {
        "larapack/dd": "^1.1",
        "phpunit/phpunit": "^9.0",
        "symfony/var-dumper": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Image\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Image\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
